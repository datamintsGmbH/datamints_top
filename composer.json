{
    "name": "datamints/datamints-top",
    "type": "typo3-cms-extension",
    "description": "Adds button to jump to the top",
    "authors": [
        {
            "name": "Mark Weisgerber",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Datamints\\DatamintsTop\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datamints\\DatamintsTop\\Tests\\": "Tests"
        }
    },
    "replace": {
        "datamints/datamints-top": "self.version",
        "typo3-ter/datamints-top": "self.version"
    }
}